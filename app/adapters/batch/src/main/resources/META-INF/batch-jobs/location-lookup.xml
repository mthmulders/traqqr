<?xml version="1.0" encoding="UTF-8"?>
<job xmlns="https://jakarta.ee/xml/ns/jakartaee"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/jobXML_2_0.xsd"
     id="example"
     restartable="false"
     version="2.0">
  <properties>
  </properties>

  <listeners>
    <listener ref="traqqrJobListener" />
  </listeners>

  <step id="step1">
    <listeners>
      <listener ref="locationLookupProcessListener"/>
      <listener ref="traqqrChunkListener" />
    </listeners>
    <chunk item-count="50">
      <reader ref="locationLookupReader" />
      <processor ref="exampleProcessor" />
      <writer ref="exampleWriter" />
    </chunk>
    <!--    <fail on="FAILED" />-->
    <!--    <end on="*" />-->
  </step>
</job>